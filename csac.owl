<?xml version="1.0"?>
<!DOCTYPE rdf:RDF [
  <!ENTITY vsto "https://vsto.org/schema#">
  <!ENTITY csac "https://vsto.org/csac#">
  <!ENTITY dcterms "http://purl.org/dc/terms/">
]>
<rdf:RDF
    xmlns="&csac;"
    xmlns:csac="&csac;"
    xmlns:vsto="&vsto;"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:dcterms="&dcterms;"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xml:base="&csac;">

  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="&vsto;"/>
  </owl:Ontology>

  <owl:Class rdf:ID="CSACmetadataService">
    <rdfs:subClassOf rdf:resource="&vsto;MetadataService"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Class that defines the possible metadata queries specific to the CSAC data archive.</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">CSAC Metadata Service</rdfs:label>
  </owl:Class>

</rdf:RDF>

