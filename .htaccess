DirectoryIndex index index.php index.html

#AddType application/rdf+xml .rdf
AddType application/rdf+xml .owl
AddType text/turtle .ttl
AddType text/n3 .n3
AddType application/n-triples .nt
AddType application/json .json

# Rewrite engine setup
RewriteEngine On

# Rewrite rule to serve HTML content
RewriteCond %{HTTP_ACCEPT} !application/rdf\+xml.*(text/html|application/xhtml\+xml)
RewriteCond %{HTTP_ACCEPT} text/html [OR]
RewriteCond %{HTTP_ACCEPT} application/xhtml\+xml [OR]
RewriteCond %{HTTP_USER_AGENT} ^Mozilla/.*
RewriteRule ^schema$ http://www.essepuntato.it/lode/owlapi/http://vsto.tw.rpi.edu/schema.owl [R=303,L]
RewriteRule ^cedar$ http://www.essepuntato.it/lode/owlapi/http://vsto.tw.rpi.edu/cedar.owl [R=303,L]
RewriteRule ^mlso$ http://www.essepuntato.it/lode/owlapi/http://vsto.tw.rpi.edu/mlso.owl [R=303,L]
RewriteRule ^csac$ http://www.essepuntato.it/lode/owlapi/http://vsto.tw.rpi.edu/csac.owl [R=303,L]

# Rewrite rule to serve RDF/XML content if requested
RewriteCond %{HTTP_ACCEPT} application/rdf\+xml
RewriteRule ^schema$ http://vsto.tw.rpi.edu/schema.owl [R=303]
RewriteRule ^cedar$ http://vsto.tw.rpi.edu/cedar.owl [R=303]
RewriteRule ^mlso$ http://vsto.tw.rpi.edu/mlso.owl [R=303]
RewriteRule ^csac$ http://vsto.tw.rpi.edu/csac.owl [R=303]

# Rewrite rule to serve RDF/XML content if requested
RewriteCond %{HTTP_ACCEPT} text/turtle
RewriteRule ^schema$ http://vsto.tw.rpi.edu/schema.ttl [R=303]
RewriteRule ^cedar$ http://vsto.tw.rpi.edu/cedar.ttl [R=303]
RewriteRule ^mlso$ http://vsto.tw.rpi.edu/mlso.ttl [R=303]
RewriteRule ^csac$ http://vsto.tw.rpi.edu/csac.ttl [R=303]

## Rewrite rule to serve n3 content if requested
RewriteCond %{HTTP_ACCEPT} text/n3
RewriteRule ^schema$ http://vsto.tw.rpi.edu/schema.n3 [R=303]
RewriteRule ^cedar$ http://vsto.tw.rpi.edu/cedar.n3 [R=303]
RewriteRule ^mlso$ http://vsto.tw.rpi.edu/mlso.n3 [R=303]
RewriteRule ^csac$ http://vsto.tw.rpi.edu/csac.n3 [R=303]

## Rewrite rule to serve n-tuples content if requested
RewriteCond %{HTTP_ACCEPT} application/n-triples
RewriteRule ^schema$ http://vsto.tw.rpi.edu/schema.nt [R=303]
RewriteRule ^cedar$ http://vsto.tw.rpi.edu/cedar.nt [R=303]
RewriteRule ^mlso$ http://vsto.tw.rpi.edu/mlso.nt [R=303]
RewriteRule ^csac$ http://vsto.tw.rpi.edu/csac.nt [R=303]

## Rewrite rule to serve json-ld content if requested
RewriteCond %{HTTP_ACCEPT} application/json
RewriteRule ^schema$ http://vsto.tw.rpi.edu/schema.json [R=303]
RewriteRule ^cedar$ http://vsto.tw.rpi.edu/cedar.json [R=303]
RewriteRule ^mlso$ http://vsto.tw.rpi.edu/mlso.json [R=303]
RewriteRule ^csac$ http://vsto.tw.rpi.edu/csac.json [R=303]

## Choose the default response
RewriteRule ^schema$ http://vsto.tw.rpi.edu/schema.owl [R=303]
RewriteRule ^cedar$ http://vsto.tw.rpi.edu/cedar.owl [R=303]
RewriteRule ^mlso$ http://vsto.tw.rpi.edu/mlso.owl [R=303]
RewriteRule ^csac$ http://vsto.tw.rpi.edu/csac.owl [R=303]

