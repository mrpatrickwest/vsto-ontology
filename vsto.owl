    <dcterms:title xml:lang="en-US">Virtual Solat Terrestrial Observatory (VSTO) ontology</dcterms:title>
    <dcterms:contributor xml:lang="en-US">Peter Fox (pfox@cs.rpi.edu)</dcterms:contributor>
