<?xml version="1.0"?>
<!DOCTYPE rdf:RDF [
  <!ENTITY vsto "http://vsto.org/schema#">
  <!ENTITY cedar "http://vsto.org/cedar#">
]>
<rdf:RDF
    xmlns="&cedar;"
    xmlns:cedar="&cedar;"
    xmlns:vsto="&vsto;"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:dc="http://purl.org/dc/terms/"
    xml:base="&cedar;">

  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="&vsto;"/>
  </owl:Ontology>

  <owl:Class rdf:ID="CEDARmetadataService">
    <rdfs:subClassOf rdf:resource="&vsto;MetadataService"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Class that defines the possible metadata queries specific to the CEDAR data archive.</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">CCEDAR Metadata Service</rdfs:label>
  </owl:Class>

</rdf:RDF>

