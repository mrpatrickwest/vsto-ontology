<?xml version="1.0"?>
<!DOCTYPE rdf:RDF [
  <!ENTITY vsto "http://vsto.org/schema#">
  <!ENTITY mlso "http://vsto.org/mlso#">
]>
<rdf:RDF
    xmlns="&mlso;"
    xmlns:mlso="&mlso;"
    xmlns:vsto="&vsto;"
    xmlns="http://escience.rpi.edu/ontology/vsto/2/0/mlso.owl#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:vsto="http://escience.rpi.edu/ontology/vsto/2/0/vsto.owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:dc="http://purl.org/dc/terms/"
    xml:base="&mlso;">

  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="&vsto;"/>
  </owl:Ontology>

  <owl:Class rdf:ID="&mlso;MLSOmetadataService">
    <rdfs:subClassOf rdf:resource="&vsto;MetadataService"/>
    <rdfs:comment>Class that defines the possible metadata queries specific to the MLSO data archive.</rdfs:comment>
    <rdfs:label>MLSO Metadata Service</rdfs:label>
  </owl:Class>

</rdf:RDF>

