                <dc:description>Magnetic Field Azimuth Angle</dc:description>
              </vsto:AzimuthAngle>
            </vsto:hasMeasuredParameter>
            <vsto:hasMeasuredParameter>
              <vsto:StokesParameterQ rdf:ID="csac_parameter_DLSP_StokesQ">
                <dc:identifier>StokesQ</dc:identifier>
                <dc:description>Stokes Q Polarization Parameter</dc:description>
                <vsto:isMeasuredBy rdf:resource="#csac_instrument_DLSP"/>
                <dc:title
                >DLSP Stokes Q</dc:title>
              </vsto:StokesParameterQ>
            </vsto:hasMeasuredParameter>
            <vsto:hasMeasuredParameter>
              <vsto:StokesParameterU rdf:ID="csac_parameter_DLSP_StokesU">
                <vsto:isMeasuredBy rdf:resource="#csac_instrument_DLSP"/>
                <dc:identifier>StokesU</dc:identifier>
                <dc:description>Stokes U Polarization Parameter</dc:description>
                <dc:title
                >DLSP Stokes U</dc:title>
              </vsto:StokesParameterU>
            </vsto:hasMeasuredParameter>
            <vsto:hasMeasuredParameter>
              <vsto:StokesParameterV rdf:ID="csac_parameter_DLSP_StokesV">
                <dc:description>Stokes v Polarization Parameter</dc:description>
                <dc:identifier>StokesV</dc:identifier>
                <vsto:isMeasuredBy rdf:resource="#csac_instrument_DLSP"/>
                <dc:title
                >DLSP Stokes V</dc:title>
              </vsto:StokesParameterV>
            </vsto:hasMeasuredParameter>
            <vsto:hasMeasuredParameter>
