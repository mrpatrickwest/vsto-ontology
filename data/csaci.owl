            <vsto:hasMeasuredParameter>
              <vsto:SourceFunction rdf:ID="csac_parameter_DLSP_SBZERO">
                <dc:description>Radiative transfer source function</dc:description>
                <dc:identifier>SBZERO</dc:identifier>
                <dc:title
                >DLSP Source Function</dc:title>
                <vsto:isMeasuredBy rdf:resource="#csac_instrument_DLSP"/>
              </vsto:SourceFunction>
            </vsto:hasMeasuredParameter>
            <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Diffraction-Limited Spectropolarimeter (DLSP) is in operation at the  National Science Foundation's Dunn Solar Telescope at the Sacramento Peak  Observatory, Sunspot, NM, as a permanent, multi-user instrument for  studying the structure and life of sunspots.</rdfs:comment>
            <vsto:hasMeasuredParameter>
              <vsto:SourceFunctionGradient rdf:ID="csac_parameter_DLSP_SB1MU">
                <dc:title
                >DLSP Source Function Gradient</dc:title>
                <dc:identifier>SB1MU</dc:identifier>
                <dc:description>Radiative transfer source function gradient</dc:description>
                <vsto:isMeasuredBy rdf:resource="#csac_instrument_DLSP"/>
              </vsto:SourceFunctionGradient>
            </vsto:hasMeasuredParameter>
          </vsto:SpectroPolarimeter>
        </vsto:servesDataFor>
        <dc:description>Community Spectro-Polarimetric Analysis Center</dc:description>
      </vsto:DataArchive>
    </vsto:dataServedBy>
    <vsto:hasMeasuredParameter>
      <vsto:AzimuthAngle rdf:ID="csac_parameter_SP4D_SAZM">
        <dc:description>Magnetic Field Azimuth Angle</dc:description>
        <dc:identifier>SAZM</dc:identifier>
        <vsto:isMeasuredBy rdf:resource="#csac_instrument_SP4D"/>
        <dc:title
        >SP4D Field Azimuth</dc:title>
      </vsto:AzimuthAngle>
    </vsto:hasMeasuredParameter>
  </vsto:SpectroPolarimeter>

</rdf:RDF>

