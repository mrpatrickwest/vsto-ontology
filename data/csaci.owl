<?xml version="1.0"?>
<!DOCTYPE rdf:RDF [
  <!ENTITY vsto "http://vsto.org/schema#">
  <!ENTITY csac "http://vsto.org/csac#">
  <!ENTITY csaci "http://vsto.org/csac/individuals/">
]>
<rdf:RDF
    xmlns="&csaci;"
    xmlns:csac="&csac;"
    xmlns:csaci="&csaci;"
    xmlns:vsto="&vsto;"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:dc="http://purl.org/dc/terms/"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xml:base="&csaci;">

  <vsto:Observatory rdf:ID="csac_observatory_Hinode">
    <dc:title>HINODE Satellite</dc:title>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >HINODE(SOLAR-B) is the third solar physics satellite of ISAS which was 
    approved as a successor of the highly successful Japan/US/UK YOHKOH 
    (SOLAR-A) collaboration. HINODE(SOLAR-B) satellite aboard the M-V Launch 
    Vehicle No. 7 (M-V-7) has been launched at 6:36 a.m. on September 23, 2006 
    (Japan Standard Time, JST) from the Uchinoura Space Center (USC). The 
    SOLAR-B was given a nickname of &amp;quot;HINODE&amp;quot; (meaning a &amp;quot;sunrize.&amp;quot;).</rdfs:comment>
    <rdfs:seeAlso xml:lang="en">
							http://www.isas.ac.jp/e/enterp/missions/solar-b/
						</rdfs:seeAlso>
  </vsto:Observatory>
  <vsto:JpgDataImage rdf:ID="csac_data_image">
    <dc:title>CSAC Jpg Data Image</dc:title>
    <dc:description>CSAC Jpg Data Image</dc:description>
  </vsto:JpgDataImage>
  <vsto:Observatory rdf:ID="csac_observatory_Sacramento_Peak">
    <dc:title>Sacramento Peak Observatory</dc:title>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >National Science Foundation's Dunn Solar Telescope at Sunspot, NM</rdfs:comment>
  </vsto:Observatory>
  <vsto:SpectroPolarimeter rdf:ID="csac_instrument_SP4D">
    <vsto:hasMeasuredParameter>
      <vsto:DopplerWidth rdf:ID="csac_parameter_SP4D_SDOP">
        <dc:identifier>SDOP</dc:identifier>
        <vsto:isMeasuredBy rdf:resource="#csac_instrument_SP4D"/>
        <dc:title>SP4D Doppler Width</dc:title>
        <dc:description>Doppler Velocity Line Width</dc:description>
      </vsto:DopplerWidth>
    </vsto:hasMeasuredParameter>
    <dc:identifier>HINODE</dc:identifier>
    <dc:description>4-D Stokes Polarimeter</dc:description>
    <vsto:hasMeasuredParameter>
      <vsto:SourceFunction rdf:ID="csac_parameter_SP4D_SBZERO">
        <dc:description>Radiative transfer source function</dc:description>
        <dc:identifier>SBZERO</dc:identifier>
        <vsto:isMeasuredBy rdf:resource="#csac_instrument_SP4D"/>
        <dc:title>SP4D Source Function</dc:title>
      </vsto:SourceFunction>
    </vsto:hasMeasuredParameter>
    <dc:title>SP4D</dc:title>
    <vsto:hasMeasuredParameter>
      <vsto:StokesParameterV rdf:ID="csac_parameter_SP4D_StokesV">
        <dc:identifier>StokesV</dc:identifier>
        <dc:description>Stokes v Polarization Parameter</dc:description>
        <vsto:isMeasuredBy rdf:resource="#csac_instrument_SP4D"/>
        <dc:title>SP4D Stokes V</dc:title>
      </vsto:StokesParameterV>
    </vsto:hasMeasuredParameter>
    <vsto:hasMeasuredParameter>
      <vsto:StokesParameterQ rdf:ID="csac_parameter_SP4D_StokesQ">
        <dc:identifier>StokesQ</dc:identifier>
        <vsto:isMeasuredBy rdf:resource="#csac_instrument_SP4D"/>
        <dc:title>SP4D Stokes Q</dc:title>
        <dc:description>Stokes Q Polarization Parameter</dc:description>
      </vsto:StokesParameterQ>
    </vsto:hasMeasuredParameter>
    <vsto:hasMeasuredParameter>
      <vsto:Polarization rdf:ID="csac_parameter_SP4D_SALPHA">
        <dc:title>SP4D Polarization</dc:title>
        <dc:identifier>SALPHA</dc:identifier>
        <vsto:isMeasuredBy rdf:resource="#csac_instrument_SP4D"/>
        <dc:description>Net Polarization</dc:description>
      </vsto:Polarization>
    </vsto:hasMeasuredParameter>
    <vsto:hasMeasuredParameter>
      <vsto:ChiSquare rdf:ID="csac_parameter_SP4D_CHISQ">
        <dc:title>SP4D Chi-Square</dc:title>
        <dc:description>Chi-square goodness of fit</dc:description>
        <vsto:isMeasuredBy rdf:resource="#csac_instrument_SP4D"/>
        <dc:identifier>CHISQ</dc:identifier>
      </vsto:ChiSquare>
    </vsto:hasMeasuredParameter>
    <vsto:hasMeasuredParameter>
      <vsto:ElevationAngle rdf:ID="csac_parameter_SP4D_SPSI">
        <dc:title>SP4D Field Inclination</dc:title>
        <vsto:isMeasuredBy rdf:resource="#csac_instrument_SP4D"/>
