        <dc:identifier>SPSI</dc:identifier>
      </vsto:ElevationAngle>
    </vsto:hasMeasuredParameter>
    <vsto:hasMeasuredParameter>
      <vsto:SourceFunctionGradient rdf:ID="csac_parameter_SP4D_SB1MU">
        <dc:description>Radiative transfer source function gradient</dc:description>
        <vsto:isMeasuredBy rdf:resource="#csac_instrument_SP4D"/>
        <dc:title>SP4D Source Function Gradient</dc:title>
        <dc:identifier>SB1MU</dc:identifier>
      </vsto:SourceFunctionGradient>
    </vsto:hasMeasuredParameter>
    <vsto:hasMeasuredParameter>
      <vsto:StokesParameterU rdf:ID="csac_parameter_SP4D_StokesU">
        <dc:title>SP4D Stokes U</dc:title>
        <dc:description>Stokes U Polarization Parameter</dc:description>
        <dc:identifier>StokesU</dc:identifier>
        <vsto:isMeasuredBy rdf:resource="#csac_instrument_SP4D"/>
      </vsto:StokesParameterU>
    </vsto:hasMeasuredParameter>
    <vsto:hasMeasuredParameter>
      <vsto:MagneticField rdf:ID="csac_parameter_SP4D_SFLD">
        <vsto:isMeasuredBy rdf:resource="#csac_instrument_SP4D"/>
        <dc:title>SP4D Field Strength</dc:title>
        <dc:identifier>SFLD</dc:identifier>
        <dc:description>Magnetic Field Strength</dc:description>
      </vsto:MagneticField>
    </vsto:hasMeasuredParameter>
    <vsto:hasMeasuredParameter>
      <vsto:DampingParameter rdf:ID="csac_parameter_SP4D_SDMP">
        <vsto:isMeasuredBy rdf:resource="#csac_instrument_SP4D"/>
        <dc:identifier>SDMP</dc:identifier>
        <dc:title>SP4D Damping Parameter</dc:title>
        <dc:description>Line Damping Parameter</dc:description>
      </vsto:DampingParameter>
    </vsto:hasMeasuredParameter>
    <vsto:hasMeasuredParameter>
      <vsto:StokesParameterI rdf:ID="csac_parameter_SP4D_StokesI">
        <dc:title>SP4D Stokes I</dc:title>
        <vsto:isMeasuredBy rdf:resource="#csac_instrument_SP4D"/>
        <dc:description>Stokes I Polarization Parameter</dc:description>
        <dc:identifier>StokesI</dc:identifier>
      </vsto:StokesParameterI>
    </vsto:hasMeasuredParameter>
    <vsto:dataServedBy>
      <vsto:DataArchive rdf:ID="csac_data_archive">
        <dc:title>CSAC</dc:title>
        <vsto:servesDataFor rdf:resource="#csac_instrument_SP4D"/>
        <vsto:servesDataFor>
          <vsto:SpectroPolarimeter rdf:ID="csac_instrument_DLSP">
            <vsto:hasMeasuredParameter>
              <vsto:ElevationAngle rdf:ID="csac_parameter_DLSP_SPSI">
                <vsto:isMeasuredBy rdf:resource="#csac_instrument_DLSP"/>
                <dc:title>DLSP Field Inclination</dc:title>
                <dc:description>Magnetic Field Inclination Angle</dc:description>
                <dc:identifier>SPSI</dc:identifier>
              </vsto:ElevationAngle>
            </vsto:hasMeasuredParameter>
            <vsto:hasMeasuredParameter>
              <vsto:DampingParameter rdf:ID="csac_parameter_DLSP_SDMP">
                <vsto:isMeasuredBy rdf:resource="#csac_instrument_DLSP"/>
