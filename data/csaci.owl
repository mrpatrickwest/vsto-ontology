                >DLSP Stokes I</dc:title>
                <dc:identifier>StokesI</dc:identifier>
                <vsto:isMeasuredBy rdf:resource="#csac_instrument_DLSP"/>
                <dc:description>Stokes I Polarization Parameter</dc:description>
              </vsto:StokesParameterI>
            </vsto:hasMeasuredParameter>
            <vsto:hasMeasuredParameter>
              <vsto:Polarization rdf:ID="csac_parameter_DLSP_SALPHA">
                <vsto:isMeasuredBy rdf:resource="#csac_instrument_DLSP"/>
                <dc:title
                >DLSP Polarization</dc:title>
                <dc:description>Net Polarization</dc:description>
                <dc:identifier>SALPHA</dc:identifier>
              </vsto:Polarization>
            </vsto:hasMeasuredParameter>
            <vsto:dataServedBy rdf:resource="#csac_data_archive"/>
            <dc:title
            >DLSP</dc:title>
            <dc:description>Diffraction-Limited Spectropolarimeter</dc:description>
            <vsto:hasMeasuredParameter>
              <vsto:AzimuthAngle rdf:ID="csac_parameter_DLSP_SAZM">
                <dc:title
                >DLSP Field Azimuth</dc:title>
                <dc:identifier>SAZM</dc:identifier>
